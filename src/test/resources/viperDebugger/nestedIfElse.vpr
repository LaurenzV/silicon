
method test_if_else(b : Int, n2 : Int)
    requires b >= n2
{
    var n: Int
    if(b >= 0){
        var ifBranch: Int := 1
        if(b >= 10){
            var innermostIf: Int := 1
            n := n2 + innermostIf + ifBranch
        }else{
            var innermostThen: Int := 1
            n := n2 + innermostThen
        }
    }else{
        var elseBranch: Int := 1
        n := n2 - elseBranch
    }

    assert n2 == n
}